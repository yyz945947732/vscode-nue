{
  "name": "nue",
  "displayName": "Nue Syntax Highlight",
  "description": "Syntax highlight for nue.js",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "text.html.nue",
        "path": "./syntaxes/nue.tmLanguage.json",
        "injectTo": [
          "text.html"
        ]
      }
    ],
    "configurationDefaults": {
      "files.associations": {
        "*.nue": "html"
      },
      "emmet.includeLanguages": {
        "nue": "html"
      },
      "html.validate.scripts": false,
      "html.validate.styles": false
    }
  }
}
