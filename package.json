{
  "name": "nue",
  "displayName": "Nue Syntax Highlight",
  "description": "Syntax highlight for nue.js",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nue",
        "aliases": [
          "Nue",
          "nue"
        ],
        "extensions": [
          ".nue"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "scopeName": "text.html.nue",
        "path": "./syntaxes/nue.tmLanguage.json",
        "injectTo": [
          "text.html"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "source.js"
        }
      }
    ],
    "configurationDefaults": {
      "files.associations": {
        "*.nue": "html"
      },
      "emmet.includeLanguages": {
        "nue": "html"
      }
    }
  }
}
